{
    "name": "nurul-komor/composer-package-starter-kit",
    "description": "",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nurul Komor",
            "email": "nurulkomor123@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "MyPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyPackage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyPackage\\MyPackageServiceProvider"
            ],
            "aliases": {
                "MyPackage": "\MyPackage\\Facades\\MyPackage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
